<odoo>
  <data>
    <record id="view_captacion_informe_provincia_tree" model="ir.ui.view">
        <field name="name">captacion.informe.provincia.tree</field>
        <field name="model">captacion.informe.provincia</field>
        <field name="arch" type="xml">
            <list>
                <field name="provincia"/>
                <field name="total"/>
                <field name="orientacion_finalizada"/>
                <field name="formacion_finalizada"/>
            </list>
        </field>
    </record>
    <record id="action_captacion_informe_provincia" model="ir.actions.act_window">
        <field name="name">Informe por Provincia</field>
        <field name="res_model">captacion.informe.provincia</field>
        <field name="view_mode">list</field>
        <field name="context">{'default_cargar_datos': True}</field>
    </record>
  </data>
</odoo>